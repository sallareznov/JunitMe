<?xml version="1.0" encoding="UTF-8"?>
<connector version="1.6" metadata-complete="false">
  <module-name/>
  <vendor-name>Tullibee</vendor-name>
  <eis-type>Interactive Brokers</eis-type>
  <resourceadapter-version>1.6</resourceadapter-version>
  <license>
    <description>GNU Lesser General Public License http://www.gnu.org/licenses/lgpl-3.0.txt</description>
    <license-required>true</license-required>
  </license>
  <resourceadapter>
    <resourceadapter-class>net.sf.tullibee.resource.TullibeeResourceAdapterImpl</resourceadapter-class>
    <config-property>
      <config-property-name>serverName</config-property-name>
      <config-property-type>java.lang.String</config-property-type>
      <config-property-value>localhost</config-property-value>
      <config-property-ignore>false</config-property-ignore>
      <config-property-supports-dynamic-updates>false</config-property-supports-dynamic-updates>
      <config-property-confidential>false</config-property-confidential>
    </config-property>
    <config-property>
      <config-property-name>portNumber</config-property-name>
      <config-property-type>java.lang.Integer</config-property-type>
      <config-property-value>4001</config-property-value>
      <config-property-ignore>false</config-property-ignore>
      <config-property-supports-dynamic-updates>false</config-property-supports-dynamic-updates>
      <config-property-confidential>false</config-property-confidential>
    </config-property>
    <outbound-resourceadapter>
      <connection-definition>
        <managedconnectionfactory-class>net.sf.tullibee.resource.ManagedConnectionFactoryImpl</managedconnectionfactory-class>
        <connectionfactory-interface>net.sf.tullibee.resource.ConnectionFactory</connectionfactory-interface>
        <connectionfactory-impl-class>net.sf.tullibee.resource.ConnectionFactoryImpl</connectionfactory-impl-class>
        <connection-interface>net.sf.tullibee.resource.Connection</connection-interface>
        <connection-impl-class>net.sf.tullibee.resource.ConnectionImpl</connection-impl-class>
      </connection-definition>
      <transaction-support>NoTransaction</transaction-support>
      <reauthentication-support>false</reauthentication-support>
    </outbound-resourceadapter>
    <inbound-resourceadapter>
      <messageadapter>
        <messagelistener>
          <messagelistener-type>net.sf.tullibee.resource.CurrentTimeListener</messagelistener-type>
          <activationspec>
            <activationspec-class>net.sf.tullibee.resource.CurrentTimeActivationSpec</activationspec-class>
          </activationspec>
        </messagelistener>
        <messagelistener>
          <messagelistener-type>net.sf.tullibee.resource.EncodedErrorListener</messagelistener-type>
          <activationspec>
            <activationspec-class>net.sf.tullibee.resource.EncodedErrorActivationSpec</activationspec-class>
          </activationspec>
        </messagelistener>
        <messagelistener>
          <messagelistener-type>net.sf.tullibee.resource.ExceptionErrorListener</messagelistener-type>
          <activationspec>
            <activationspec-class>net.sf.tullibee.resource.ExceptionErrorActivationSpec</activationspec-class>
          </activationspec>
        </messagelistener>
        <messagelistener>
          <messagelistener-type>net.sf.tullibee.resource.RealTimeBarListener</messagelistener-type>
          <activationspec>
            <activationspec-class>net.sf.tullibee.resource.RealTimeBarActivationSpec</activationspec-class>
          </activationspec>
        </messagelistener>
      </messageadapter>
    </inbound-resourceadapter>
  </resourceadapter>
</connector>
